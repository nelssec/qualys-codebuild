{
  "name": "qualys-codebuild",
  "version": "1.0.0",
  "description": "Qualys QScanner integration for AWS CodeBuild",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "container-scan": "node dist/container-scan/index.js",
    "code-scan": "node dist/code-scan/index.js"
  },
  "keywords": [
    "qualys",
    "qscanner",
    "aws",
    "codebuild",
    "security",
    "vulnerability",
    "container",
    "sca"
  ],
  "author": "Qualys",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "prettier": "^3.1.0"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.450.0",
    "@aws-sdk/client-s3": "^3.450.0",
    "@aws-sdk/client-eventbridge": "^3.450.0",
    "@aws-sdk/client-securityhub": "^3.450.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
